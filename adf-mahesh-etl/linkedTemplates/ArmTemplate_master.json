{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-mahesh-etl"
		},
		"AzureBlobStorage1httpendpoint_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1httpendpoint'"
		},
		"datalakegit_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakegit'"
		},
		"ls_dbs_notebook_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_dbs_notebook'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azmahesh-kv.vault.azure.net/"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.github.com/repos/narem445/DE-DataEngineering-/contents/dgda_drug_database_data"
		},
		"HttplinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.github.com/repos/narem445/DE-DataEngineering-/contents/dgda_drug_database_data"
		},
		"datalakegit_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://awsstoragedatalaketest.dfs.core.windows.net/"
		},
		"ls_Http_files_copy_from_git_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.github.com/"
		},
		"ls_http_git_narem445_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"ls_kaggle_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.kaggle.com"
		},
		"ls_narem_git_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-mahesh-etl_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1httpendpoint_connectionString": {
						"value": "[parameters('AzureBlobStorage1httpendpoint_connectionString')]"
					},
					"datalakegit_accountKey": {
						"value": "[parameters('datalakegit_accountKey')]"
					},
					"ls_dbs_notebook_accessToken": {
						"value": "[parameters('ls_dbs_notebook_accessToken')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"HttplinkedService_properties_typeProperties_url": {
						"value": "[parameters('HttplinkedService_properties_typeProperties_url')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"datalakegit_properties_typeProperties_url": {
						"value": "[parameters('datalakegit_properties_typeProperties_url')]"
					},
					"ls_Http_files_copy_from_git_properties_typeProperties_url": {
						"value": "[parameters('ls_Http_files_copy_from_git_properties_typeProperties_url')]"
					},
					"ls_http_git_narem445_properties_typeProperties_url": {
						"value": "[parameters('ls_http_git_narem445_properties_typeProperties_url')]"
					},
					"ls_kaggle_http_properties_typeProperties_url": {
						"value": "[parameters('ls_kaggle_http_properties_typeProperties_url')]"
					},
					"ls_narem_git_properties_typeProperties_url": {
						"value": "[parameters('ls_narem_git_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-mahesh-etl_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-mahesh-etl_ArmTemplate_0')]"
			]
		}
	]
}